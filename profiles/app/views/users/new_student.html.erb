<%= form_for @user do |f| %>
<%= f.text_field :f_name %>
<%= f.text_field :l_name %>
<%= f.text_field :org_name %>
<%= f.text_field :email %>
<%= f.text_field :image_src %>
<%= f.text_field :phone %>
<%= f.text_field :city %>
<%= f.text_field :state %>
<%= f.text_field :website %>
<%= f.text_field :github %>
<%= f.text_field :twitter %>
<%= f.text_field :linkedin %>
<%= f.text_field :behance %>
<%= f.text_field :bio %>
<%= f.submit %>